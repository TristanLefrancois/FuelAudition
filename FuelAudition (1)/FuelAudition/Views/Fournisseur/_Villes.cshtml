@model FuelAudition.Models.ViewModel.FournisseurDetailVM

@if (Model.Villes.Any())
{
    @Html.LabelFor(model => model.VilleId, new { @class = "control-label" })

    @Html.DropDownListFor(model => model.VilleId, Model.Villes, new { @class = "form-control" })
    @Html.ValidationMessageFor(model => model.VilleId, "", new { @class = "text-danger" })

}

    


